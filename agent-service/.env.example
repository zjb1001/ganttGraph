# Gantt Graph AI Agent Service Configuration
# =========================================

# LLM Provider: openai, zhipu, deepseek, ollama
LLM_PROVIDER=zhipu

# API Keys (choose based on provider)
ZHIPU_API_KEY=your-zhipu-api-key-here
# OPENAI_API_KEY=your-openai-api-key-here
# DEEPSEEK_API_KEY=your-deepseek-api-key-here

# Common API key (used if provider-specific not set)
LLM_API_KEY=your-api-key-here

# Base URL (auto-set based on provider, can override)
# LLM_BASE_URL=https://open.bigmodel.cn/api/paas/v4

# Model name
LLM_MODEL=glm-4-flash

# Service port
PORT=8000
